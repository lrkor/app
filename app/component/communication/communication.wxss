page{
    background-color: #f7f7f7;
  }

.choose_unit,.person_unit{
    background-color: #ffffff;
    margin-bottom: 20rpx;
}
.choose_unit{
    margin-top: 20rpx;
}

.list {
    border-top: 1px solid #F1F1F1;  
}
.list_item{
    padding-left: 30rpx;
}
.list_item_child{
    display: flex;
    padding: 15px 0;
    border-bottom: 1px solid #f1f1f1;
    justify-content: space-between;
}
.list_item:last-child .list_item_child{
    border: none;
}
.list_item_child_left{
    display: flex;
    align-items: center;
}
.list_item_child_right{
    padding-right: 30rpx;
}
.org_name{
    font-size: 30rpx;
    color: #333;
}
.org_img{
    width: 37rpx;
    height: 40rpx;
    margin-right: 20rpx;
}
.right_img{
    width: 14rpx;
    height: 25rpx;
}
.communicatePerson{
    height: 140rpx;
}
.person_container{
    display: flex;
    justify-content: space-between;
    background-color: #ffffff;
}
.person_name{
    margin-top: 5rpx;   
    height: 30rpx;
    font-size: 32rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: #333333;
    line-height: 80rpx;
}
.person_phone{
    width: 185rpx;
    height: 21rpx;
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: #999999;
    line-height: 80rpx;
    margin-right: 20rpx;
}
.person_position{
    width: 90rpx;
    height: 27rpx;
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: 500;
    color: #999999;
    line-height: 80rpx;
    border-left: 1px solid #999999;
    padding-left: 20rpx;
    margin-right: 164rpx;
}
.person_center{
    display: flex;
    flex: 2;
    flex-direction: column;
    justify-content:space-between;
    border-bottom: 1px solid #f1f1f1;
}
.person_center view{
    margin-top: 20rpx;
}
.person_left{
    display: flex;
    width: 140rpx;
    align-items: center;
    justify-content: center;
}

.person_left image{
    width: 80rpx;
    height: 80rpx;
}
.person_right{
    width: 110rpx;
    border-bottom: 1px solid #f1f1f1;
    background-image: url('data:image/png;base64,');
    background-size: 48rpx 48rpx;
    background-repeat:no-repeat;
    background-position: center;
}

.person_right_hover{
    background-image: url('data:image/png;base64,');
}

.bc_f7{
    background-color: #f3f3f3;
}
.person_right image{
    width: 48rpx;
    height: 48rpx;
    margin-top: 47rpx;
}