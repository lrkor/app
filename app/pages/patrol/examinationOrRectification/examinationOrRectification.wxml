<view class="check_list">
    <view class="list">
        <block wx:for="{{list}}" wx:key="item.index">
        <view class="check_item" data-id="{{item.id}}" bind:tap="goDetail">
            <view class="item_top">
                <view class="item_top_title">
                    <view class="number">{{item.number}}</view>
                    <view class="isSend" wx:if="{{type==1}}">待审批</view>
                    <view class="isSend unSend" wx:else>待整改</view>
                </view>
                <view class="check_type"><view class="check_type_title">检查项：</view><view class="check_type_val">{{item.check_type}}</view></view>
                <view class="type"><view class="type_title">类<view class="empty">查</view>型：</view><view class="type_val">{{item.type}}</view></view>
                <view class="describe"><view class="describe_title">描<view class="empty">查</view>述：</view><view class="describe_val">{{item.describe}}</view></view>
            </view>
            <view class="item_down">
                <view class="results">整改人：{{item.createPeople}}</view>
                <view class="create">
                    <view class="createPeople">整改期限：</view>
                    <view class="createDate">{{item.createDate}}</view>
                    <view class="createTime">{{item.createTime}}</view>
                    <view class="isOverdue">逾期</view>
                </view>
            </view>
        </view>
        </block>
    </view>
</view>